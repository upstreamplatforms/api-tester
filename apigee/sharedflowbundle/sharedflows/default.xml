<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SharedFlow name="default">
  <Step>
    <Name>JS-SetTestId</Name>
    <Condition>upstream.testId = null</Condition>
  </Step>
  <Step>
    <Name>SC-GetTestCases</Name>
    <Condition>upstream.testId != null and upstream.testCase = null</Condition>
  </Step>
  <Step>
    <Name>JS-SetTestData</Name>
    <Condition>upstream.testId != null and currentstep.flowstate = "PROXY_REQ_FLOW"</Condition>
  </Step>
  <Step>
    <Name>JS-RunTest</Name>
    <Condition>upstream.testId != null and currentstep.flowstate = "RESP_SENT"</Condition>
  </Step>
  <Step>
    <Name>SC-SaveResults</Name>
    <Condition>upstream.testId != null and currentstep.flowstate = "RESP_SENT"</Condition>
  </Step>
</SharedFlow>
